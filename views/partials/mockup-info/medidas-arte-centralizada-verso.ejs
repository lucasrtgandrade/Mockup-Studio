<div class="label-input"
     id="arteCentralizada--verso">
    <h2>Arte Verso Centralizado</h2>
    <div class="label-input__elementos">
        <label for="alturaCentralizada--verso">Altura da arte:</label>
        <input type="number"
               id="alturaCentralizada--verso"
               data-max-altura="<%= dimensoes.Altura %>"
               data-altura-copo="<%= dimensoes.AlturaCopo %>">

        <!--                Feedback Altura Verso-->
        <div class="label-input__feedback">
            <img src="imagens/icones/info.svg"
                 alt="icone de informação"
                 id="alturaIcone">
            <span id="feedbackAlturaVerso">Altura máx de <%= dimensoes.Altura%>mm</span>
        </div>
        <!--            Largura-->
        <label for="larguraCentralizada--verso">Largura da arte:</label>
        <input type="number"
               id="larguraCentralizada--verso"
               data-max-largura="<%= dimensoes.Largura %>">
        <!--Feedback largura -->
        <div class="label-input__feedback">
            <img src="imagens/icones/info.svg"
                 alt="icone de informação"
                 id="larguraIcone">
            <span id="feedbackLarguraVerso">Largura máx de <%= dimensoes.Largura%>mm</span>
        </div>
    </div>


    <!--               Switch Descentralizar -->
    <div class="switch--elementos">
        <div class="switch__header">
            <h2>Descentralizada:</h2>
            <p><%= projecaoAprovacao.AvisoDescentralizado %></p>
        </div>
        <label class="switch-c qr-code-switch">
            <input type="checkbox" id="alternarCalculo">
            <span class="puxador redondo"></span>
        </label>
    </div>

    <script>

    </script>
</div>
